{
  "name": "power-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev -r dotenv/config ./src/index.ts",
    "docker:build": "docker build -t power-api .",
    "docker:start": "docker-compose up -d",
    "generate": "./node_modules/.bin/prisma generate",
    "prisma": "prisma migrate dev --preview-feature",
    "seed": "ts-node-dev -r dotenv/config migration/seed",
    "start": "node -r dotenv/config ./dist/index.js"
  },
  "dependencies": {
    "@prisma/cli": "^2.13.1",
    "@prisma/client": "^2.13.1",
    "aws-sdk": "^2.820.0",
    "bcryptjs": "^2.4.3",
    "busboy": "^0.3.1",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.5",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "mime-types": "^2.1.28",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/busboy": "^0.2.3",
    "@types/inversify": "^2.0.33",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.19",
    "tslib": "^2.0.3",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
